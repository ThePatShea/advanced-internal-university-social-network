/***************         CSS Reset         ***************/
.gldp-flatwhite
  .dow
    cursor               default !important  // Overrides the datepicker framework's 'wait' cursor

button
  line-height            normal
  display                block
  background             none
  border                 0

pre
  word-break             normal
  word-wrap              normal
  border-radius          0
  background             0
  padding                0
  margin                 0
  border                 0

hr
  height           1px
  border           0
  margin           0

a:visited,
a:hover,
a
  text-decoration        none

.row-fluid
  width                  auto

h1,
h2,
h3,
h4,
h5,
h6
  line-height            100%
  margin                 0

input
  border                 0
  border-radius          0
  border-box             0

textarea
  resize                 none
  border-radius          0
  border-box             0
  border                 0

section,
button,
input,
div,
pre,
a
  border-box             0

body
  overflow-y hidden
  @media (max-width: 767px)
    padding 0

.navbar
  margin               0
  .navbar-inner
    border-radius          0
    border                 0
    a
      text-shadow              0 0
    .cb-logo-main,
    .brand
      padding                  0
      margin                   0
    .nav
      & > li
        a
          text-shadow            0 0
