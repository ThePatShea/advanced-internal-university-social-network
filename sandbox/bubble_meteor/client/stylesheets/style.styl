/***************         CSS Reset         ***************/
.gldp-flatwhite
  .dow
    cursor               default !important  // Overrides the datepicker framework's 'wait' cursor

button
  line-height            normal
  display                block
  background             none
  border                 0

a:visited,
a:hover,
a
  text-decoration        none

.row-fluid
  width                  auto

h1,
h2,
h3,
h4,
h5,
h6
  line-height            100%
  margin                 0

section,
button,
input,
div,
a
  border-box             0

.navbar
  .navbar-inner
    border-radius          0
    border                 0
    a
      text-shadow              0 0
    .nav
      & > li
        a
          text-shadow            0 0
/***************         CSS Reset         ***************/



/***************  Multi-Purpose Elements   ***************/
/**********  Color Palette  **********/
/*****    Standard White    *****/
std-white-light-color(n)
  color                  #F7F8F8
std-white-light-fill(n)
  fill                   #F7F8F8
std-white-light-bg(n)
  background             #F7F8F8

std-white-dark-color(n)
  color                  #EEF0F0
std-white-dark-fill(n)
  fill                   #EEF0F0
std-white-dark-bg(n)
  background             #EEF0F0
/*****    Standard White    *****/


/*****      Main Color      *****/
main-xdark-color(n)
  color                  #011126
main-xdark-fill(n)
  fill                   #011126
main-xdark-bg(n)
  background             #011126


main-dark-color(n)
  color                  #001f37
main-dark-fill(n)
  fill                   #001f37
main-dark-bg(n)
  background             #001f37


main-light-stroke(n)
  path
    stroke                   #2f5973
main-light-color(n)
  color                  #2f5973
main-light-fill(n)
  fill                   #2f5973
main-light-bg(n)
  background             #2f5973


main-xlight-border(n)
  border-thin            #95afbf
main-xlight-color(n)
  color                  #95afbf
main-xlight-fill(n)
  fill                   #95afbf
main-xlight-bg(n)
  background             #95afbf
/*****      Main Color      *****/


