/***************         CSS Reset         ***************/
button
  line-height            normal
  display                block
  background             none
  border                 0

button,
div
  border-box             0

h1, h2, h3, h4, h5, h6
  line-height            100%
  margin                 0
/***************         CSS Reset         ***************/



/***************  Multi-Purpose Elements   ***************/
