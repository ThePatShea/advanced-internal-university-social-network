/**********  Color Palette  **********/


/**********  Font Palette   **********/
/*****    Main Font     *****/
font-main(n)
  font-family            'brandon-grotesque', Arial, Sans-Serif
font-main-black(n)
  font-weight            900
  font-main              0
font-main-bold(n)
  font-weight            700
  font-main              0
font-main-medium(n)
  font-weight            500
  font-main              0
font-main-regular(n)
  font-weight            400
  font-main              0

font-main-black-11-u(n)
  text-transform         uppercase
  font-size              11px
  font-main-black        0
/*****    Main Font     *****/

/*****  Alternate Font  *****/
font-alt(n)
  font-family            'proxima-nova', Arial, Sans-Serif
font-alt-black(n)
  font-weight            900
  font-alt               0
font-alt-extrabold(n)
  font-weight            800
  font-alt               0
font-alt-bold(n)
  font-weight            700
  font-alt               0
font-alt-semibold(n)
  font-weight            600
  font-alt               0
font-alt-regular(n)
  font-weight            400
  font-alt               0
font-alt-light(n)
  font-weight            300
  font-alt               0
/*****  Alternate Font  *****/
/**********  Font Palette   **********/


/**********   Properties    **********/
border-thin-top-btm(color)
  border-bottom          1px solid color
  border-top             1px solid color

border-thin(color)
  border                 1px solid color

border-box(n)
  -webkit-box-sizing     border-box
  -moz-box-sizing        border-box
  box-sizing             border-box

box-shadow(args...)
  -webkit-box-shadow     args
  -moz-box-shadow        args
  box-shadow             args

border-radius(n)
  -webkit-border-radius  n
  -moz-border-radius     n
  border-radius          n
/**********   Properties    **********/


/**********     Helpers     **********/
/*****  Vertical Middle  *****/
.v-middle
  display                table
  height                 100%
  .v-inner
    display                table-cell
    vertical-align         middle
/*****  Vertical Middle  *****/
/**********     Helpers     **********/


/**********     Buttons     **********/
std-btn(n)
  text-transform         uppercase
  padding                8px 15px
  border-radius          15px
  font-size              11px
  main-xlight-border     0

.std-btn-light
  main-light-color       0
  font-main-bold         0
  std-btn                0

.std-btn-dark
  background             rgba(0,0,0,.35)
  main-xlight-color      0
  font-main-black        0
  std-btn                0
/**********     Buttons     **********/


/**********      Icons      **********/
svg
  vertical-align         top

.cb-icon-light-stroke
  main-light-stroke      0

.cb-icon-light-fill
  main-light-fill        0

.cb-icon-xlight
  main-xlight-fill       0
/**********      Icons      **********/
/***************  Multi-Purpose Elements   ***************/



/***************  Single-Purpose Elements  ***************/
/**********       sidebar        **********/
.sidebar
  height                 5000px
  width                  240px
  position               fixed
  main-light-bg          0
  .top-btns
    border-thin-top-btm      #BBCCD7
    overflow                 hidden
    .top-btn
      height                     50px
      float                      left
      width                      50%
      font-main-black-11-u       0
      main-light-color           0
      main-xlight-bg             0
      &.left
        border-right                 1px solid #7C9CAF
      .lbl
        padding                      0 0 0 10px
        line-height                  20px
      &:hover
        text-decoration              underline
        background                   #B7C8D3
  .main-btns
    .main-btn
      box-shadow                 0 1px #386C8B
      padding                    0 0 0 15px
      position                   relative
      display                    block
      text-align                 left
      height                     50px
      font-size                  15px
      width                      100%
      z-index                    20
      std-white-light-color      0
      font-alt-light             0
      &:hover
        text-decoration              underline
        position                     relative
        background                   #274B61
        z-index                      10
/**********       sidebar        **********/

/**********        bubble-cover       **********/
.bubble-cover
  background             #333 // TESTING
  height                 145px
  padding                0 30px
  .bubble-name
    color                    white
    font-size                37px
    font-alt-light           0
  .bubble-subtitle
    text-transform           uppercase
    font-size                12px
    main-xlight-color        0
    font-main-bold           0
  .std-btn-dark
    margin                   5px 0
    float                    right
    clear                    right
    .lbl
      padding                    0 0 0 5px
/**********        bubble-cover       **********/

/**********  bubble-section-selector  **********/
.bubble-section-selector
  height                 100px
  std-white-light-fill   0
  .btn-bubble-section
    height                   100px
    width                    100px
    float                    left
  .btn-bubble-section:nth-child(1)
    std-white-light-bg       0
  .btn-bubble-section:nth-child(3)
    opacity                  .85
  .btn-bubble-section:nth-child(4)
    opacity                  .70
  .btn-bubble-section:nth-child(5)
    opacity                  .55
  .btn-bubble-section:nth-child(6)
    opacity                  .40
/**********  bubble-section-selector  **********/
/***************  Single-Purpose Elements  ***************/
