/**********    Functions    **********/
fontize(family = auto, weight = auto, val-transform = auto, val-size = auto, val-line-height = auto, val-letter-spacing = auto, val-text-decoration = auto, val-text-align = auto)
  if family == main
    font-family    'brandon-grotesque', Arial, Sans-Serif
  else if family == alt
    font-family    'proxima-nova', Arial, Sans-Serif
  if weight == black
    font-weight    900
  else if weight == bold
    font-weight    700
  else if weight == medium
    font-weight    500
  else if weight == regular
    font-weight    400
  else if weight == light
    font-weight    300
  else if weight == thin
    font-weight    100
  text-decoration    val-text-decoration
  letter-spacing     val-letter-spacing
  line-height        val-line-height
  text-align         val-text-align
  text-transform     val-transform
  font-size          val-size

breakpoint-swap(property, val-desktop, val-mobile)
  @media (min-width: 768px)
    if property == display
      display    val-desktop
    if property == padding
      padding    val-desktop
  @media (max-width: 767px)
    if property == display
      display    val-mobile
    if property == padding
      padding    val-mobile

positionize(val-height = auto, val-width = auto, val-padding = auto, val-margin = auto, val-border-radius = auto, val-float = auto, val-move-position = auto, val-display = auto, val-overflow = visible)
  border-radius     val-border-radius
  overflow          val-overflow
  padding           val-padding
  display         bord  val-display
  margin            val-margin
  height            val-height
  width             val-width
  float             val-float
  border-box        0

border-spin(size-top, size-right, size-bottom, size-left, color-top, color-right, color-bottom, color-left)
  border-bottom     size-bottom solid color-bottom
  border-right      size-right solid color-right
  border-left       size-left solid color-left
  border-top        size-top solid color-top

move-position(val-position = relative, val-top = auto, val-right = auto, val-bottom = auto, val-left = auto)
  position          val-position
  bottom            val-bottom
  right             val-right
  left              val-left
  top               val-top

colorize(val-color = auto, val-background = auto, val-fill = auto, val-border = auto)
  background        val-background
  border            val-border
  color             val-color
  fill              val-fill
  stroke            val-fill
  path
    stroke          val-fill
  line
    stroke          val-fill

generate-button(val-colorize, val-fontize, val-border-radius, val-padding = 0, val-margin = 0)
  border-radius     val-border-radius
  colorize          val-colorize
  fontize           val-fontize
  padding           val-padding
  text-transform    uppercase
  line-height       100%
